<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.dao.DeviceMapper">

    <select id="selectActiveIpAndPortByTcpModel" resultMap="selectActiveIpAndPortMap">
        select ip, port
        from device
        where tcp_model = #{model}
          and device_status = 0
          and deleted = false
    </select>
    <resultMap id="selectActiveIpAndPortMap" type="com.cn.common.core.KeyValue">
        <result property="key" column="ip"/>
        <result property="value" column="port"/>
    </resultMap>
</mapper>
